    <single_statement>Y</single_statement>
    <sql>INSERT INTO fato_pedidos(id_pedido, sk_pessoa, sk_produto, dispositivo, geohash, telefone, dt_venda, qtde, valor_unit, total) SELECT s.id_pedido, d.sk_pessoa, dp.sk_produto, s.dispositivo, s.geohash, s.telefone, s.dt_venda, s.qtde, s.valor_unit, s.valor_total FROM stg_pedidos s INNER JOIN dim_pessoas d ON d.id = s.id_pessoa INNER JOIN dim_produtos dp ON dp.id = s.id_produto WHERE NOT EXISTS (SELECT 1 FROM fato_pedidos f WHERE s.id_pedido = s.id_pedido);</sql>
